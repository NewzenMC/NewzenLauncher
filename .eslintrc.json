{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["standard"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-extra-semi": "warn",
        "semi": "warn",
        "eol-last": "off",
        "no-undef": "off",
        "indent": "off",
        "spaced-comment": "off",
        "no-tabs": "warn",
        "space-before-function-paren": "off",
        "quotes": "off",
        "no-throw-literal": "off",
        "curly": "off"
    }
}
