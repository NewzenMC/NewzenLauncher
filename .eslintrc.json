{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-extra-semi": "warn",
        "semi": "warn",
        "eol-last": "off",
        "no-undef": "off",
        "indent": "off",
        "spaced-comment": "off",
        "no-tabs": "warn",
        "space-before-function-paren": "off",
        "quotes": "off",
        "no-throw-literal": "off",
        "curly": "off",
        "new-cap": "off",
        "prefer-const": "off",
        "camelcase": "off",
        "handle-callback-err": "off",
        "prefer-promise-reject-errors": "off",
        "no-async-promise-executor": "off",
        "no-template-curly-in-string": "off",
        "no-useless-call": "off",
        "no-control-regex": "off",
        "eqeqeq": "off",
        "no-unused-vars": "warn",
        "node/handle-callback-err": "off",
        "no-void": "off",
        "no-labels": "off",
        "node/no-deprecated-api": "off",
        "no-unused-expressions": "off"
    }
}